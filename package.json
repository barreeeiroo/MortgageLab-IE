{
	"name": "mortgagelab-ie",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"postbuild": "cp -r data dist/data",
		"preview": "astro preview",
		"astro": "astro",
		"typecheck": "astro check",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"test": "vitest",
		"test:unit": "vitest run src",
		"test:integration": "vitest run tests/integration",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:run": "bun run test:unit && bun run test:integration",
		"test:coverage": "vitest run src --coverage",
		"check": "bun run lint && bun run typecheck && bun run test:run",
		"rates:scrape": "bun run scripts/rate-scraper/index.ts",
		"rates:scrape-all": "bun run scripts/rate-scraper/index.ts --all",
		"rates:validate": "bun run scripts/rate-scraper/validator.ts"
	},
	"dependencies": {
		"@astrojs/react": "^4.4.2",
		"@astrojs/sitemap": "^3.6.1",
